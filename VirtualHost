<VirtualHost *:80>
    ServerName data-analytics-tool
    ServerAlias dev.data-analytics-tool
    ServerAlias localhost
    DocumentRoot /var/www/symfony-boilerplate
    <Directory /var/www/symfony-boilerplate>
        # enable the .htaccess rewrites
        AllowOverride All
        Order allow,deny
        Allow from All
    </Directory>
    ErrorLog /var/log/apache2/symfony-boilerplate.log
    CustomLog /var/log/apache2/symfony-boilerplate.log combined
</VirtualHost>